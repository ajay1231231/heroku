    "dotenv": "4.0.0",
    "eslint": "4.4.1",
    "eslint-config-react-app": "^2.0.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.35.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.1.0",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "firebase": "^5.2.0",
    "fs-extra": "3.0.1",
    "google-map-react": "^1.0.5",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "lightbox-react": "^0.1.6",
    "mobx": "^4.3.1",
    "mobx-react": "^5.2.3",
    "object-assign": "4.1.1",
    "portfolio-card": "^1.0.6",
    "postcss-flexbugs-fixes": "3.2.0",
    "promise": "8.0.1",
    "prop-types": "^15.6.2",
    "react": "^15.6.1",
    "react-awesome-button": "^4.2.1",
    "react-bootstrap-carousel": "^3.1.0",
    "react-bootstrap-table-next": "^0.1.15",
    "react-collapsible-component": "^1.1.0",
    "react-dev-utils": "^4.0.1",
    "react-dom": "^15.6.1",
    "react-facebook-login": "^4.0.1",
    "react-firebase-schema-form": "^1.1.1",
    "react-flipcard-2": "^0.3.2",
    "react-google-login": "^3.2.1",
    "react-grid-gallery": "^0.5.1",
    "react-iframe": "^1.2.0",
    "react-image-gallery-lightbox": "0.0.30",
    "react-image-slider": "^0.1.0",
    "react-lightbox-component": "^1.2.1",
    "react-modal": "^3.5.1",
    "react-parallax": "^1.8.0",
    "react-photo-gallery": "^6.0.28",
    "react-responsive-carousel": "^3.1.41",
    "react-router-dom": "^4.3.1",
    "react-slick": "^0.23.1",
    "react-slick-slider": "^0.15.10",
    "react-table": "^6.8.6",
    "react-tabs": "^2.2.2",
    "react-viewport-slider": "^1.0.0",
    "react-youtube": "^7.6.0",
    "reactstrap": "^6.3.0",
    "recompose": "^0.25.0",
    "style-loader": "0.18.2",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.5.9",
    "webpack": "3.5.1",
    "webpack-dev-server": "2.7.1",
    "webpack-manifest-plugin": "1.2.1",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "plugins": [
      "transform-decorators-legacy"
    ],
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "node-sass": "^4.9.2",
    "postcss-loader": "^2.1.6",
    "react-alice-carousel": "^1.10.3",
    "sass-loader": "^7.0.3"
  }
}
